options:

    -?, --help        
        --usage       
    -V, --version

    -f, --file=NOMDEFICHIER               

    -e, --export-png=NOMDEFICHIER         
    -a, --export-area=x0:y0:x1:y1     
    -C, --export-area-page
    -D, --export-area-drawing
        --export-area-snap
    -i, --export-id=ID     
    -j, --export-id-only     

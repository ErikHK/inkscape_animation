# Convenience stub makefile to call the real Makefile.

@SET_MAKE@

OBJEXT = @OBJEXT@

# Explicit so that it's the default rule.
all:
	cd ../.. && $(MAKE) extension/internal/all

clean %.a %.$(OBJEXT):
	cd ../.. && $(MAKE) extension/internal/$@

.PHONY: all clean

.SUFFIXES:
.SUFFIXES: .a .$(OBJEXT)
